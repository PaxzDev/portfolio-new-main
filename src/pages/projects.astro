---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';

---

<Layout title="Main - paxzdev.xyz">

	<nav class="bg-gradient-to-r from-neutral-900 to-neutral-800 text-white py-2">
		<div class="max-w-6xl mx-auto px-4 flex items-center justify-between">
			<div class="flex items-center">
				<span class="font-semibold text-xl"><span class="bg-gradient-to-r from-blue-600 via-blue-500 to-blue-400 inline-block font-bold text-transparent bg-clip-text">paxzdev</span>.xyz</span>
			</div>
	
			<!-- Hamburger menu for small screens -->
			<div class="lg:hidden">
				<button id="nav-toggle" class="text-white duration-150 hover:text-blue-500 focus:outline-none">
					<svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
						<path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5M12 17.25h8.25" />
					</svg>
				</button>
			</div>
	
			<ul class="hidden lg:flex justify-center space-x-6 text-gray-300">
				<li><a href="/" class="font-semibold duration-200 hover:text-blue-500">Home</a></li>
				<li><a href="/about" class="font-semibold duration-200 hover:text-blue-500">About</a></li>
				<li><a href="/projects" class="font-semibold duration-200 hover:text-blue-500">Projects</a></li>
				<li><a href="/contact" class="font-semibold duration-200 hover:text-blue-500">Contact</a></li>
			</ul>
	
			<a class="rounded bg-gradient-to-r from-blue-600 via-blue-500 to-blue-400 inline-block px-6 py-3 text-sm font-medium text-white shadow-sm shadow-blue-500/50 duration-150 hover:shadow-lg hover:shadow-blue-500/50 flex items-center justify-center gap-2" href="/projects">
				Projects
			</a>
		</div>
	</nav>
	








	<main class="flex items-center justify-center w-full py-12 overflow-y: auto bg-gradient-to-t from-slate-900 to-blue-950" id="main">
		<div class="max-w-6xl mx-auto px-4 text-center">
		  <h1 class="text-white font-semibold text-3xl">
			A collection of websites i've made to get a perspective.
		  </h1>
		  <p class="font-bold text-gray-400 text-sm mt-1">All websites shown are coded and designed by me. <br>Dont mind the music playing in the background, my bad.</p>
	  
		  <!-- Add a grid container -->
		  <div class="grid grid-cols-3 gap-4 justify-items-center mt-8">
			<!-- Video 1 -->
			<div class="w-full h-64">
			  <video class="w-full h-full object-cover rounded-lg" poster="" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609194770599988/Desktop_2024.05.01_-_17.54.00.03.mp4?ex=6640dbbc&is=663f8a3c&hm=ef484f32b757d154c2e763929113d49d0cfccf190ea49803b3251fb2649209ae&" controls muted >
				
			  </video>
			</div>
	  
			<!-- Video 2 -->
			<div class="w-full h-64">
			  <video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609189825380353/Desktop_2024.01.07_-_20.28.10.02.mp4?ex=6640dbbb&is=663f8a3b&hm=165c06e2b510a9429c57fd62da9d466bb077cabc4c0a374b7784bf02bc2bfc9b&" poster="" controls muted ></video>
			</div>
	  
			<!-- Video 3 -->
			<div class="w-full h-64">
			  <video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609191012634696/Desktop_2024.03.13_-_20.32.55.02.mp4?ex=6640dbbb&is=663f8a3b&hm=8f8078b8f55a2307a2efa07ee9fdccdc5ce712a4f0de658a29d8d771443043f3&" poster="" controls muted ></video>
			</div>

			<!-- Video 3 -->
			<div class="w-full h-64">
			  <video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609187002749020/Desktop_2024.01.03_-_22.14.08.01.mp4?ex=6640dbba&is=663f8a3a&hm=d1ca282ca297fc53c62faa5cdb52b7ab4b565f96e6e5bb42db393bbf80e168ca&" poster="" controls muted ></video>
			</div>

			<div class="w-full h-64">
				<video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609193302458439/Desktop_2024.05.01_-_17.48.32.02.mp4?ex=6640dbbc&is=663f8a3c&hm=a6f9d1d51a091d477a178b9f6311907d437f15ab2d9106c717309efe54027ba9&" poster="" controls muted ></video>
			</div>

			<div class="w-full h-64">
				<video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609192329515108/Desktop_2024.04.25_-_21.12.08.01.mp4?ex=6640dbbb&is=663f8a3b&hm=0e106ecd7522c452dac7693e0c782148c7668a6415e9e40164e2ad92e241fdfe&" poster="" controls muted></video> 
			</div>

			
			<div class="w-full h-64">
				<video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235609188487397446/Desktop_2024.01.06_-_16.59.43.02.mp4?ex=6640dbbb&is=663f8a3b&hm=3dcf40d4656c4b9ebc61a324dc72ec5b1aff12b47a81dbe149643e27783d41bf&" poster="" controls muted></video> 
			</div>

			
			<div class="w-full h-64">
				<video class="w-full h-full object-cover rounded-lg" src="https://cdn.discordapp.com/attachments/1235607953260810290/1235614476393250927/Desktop_2024.05.02_-_17.28.48.02.mp4?ex=6640e0a7&is=663f8f27&hm=ff7d20622b6d607812341e5eceffd3a7a8bc623547e13e8492872cf7d9155a5e&" poster="" controls muted></video> 
			</div>

			<div class="w-full h-64">
				<video class="w-full h-full object-cover rounded-lg" src="" poster="" controls muted></video> 
			</div>
		  </div>

		  
	  
		</div>
	  </main>








	  
	<footer class="bg-gray-100">
		<div class="relative mx-auto max-w-screen-xl px-4 py-16 sm:px-6 lg:px-8 lg:pt-24">
		  <div class="absolute end-4 top-4 sm:end-6 sm:top-6 lg:end-8 lg:top-8">
			<a
			  class="inline-block rounded-full bg-gradient-to-r from-blue-600 via-blue-500 to-blue-400 p-2 text-white shadow transition hover:bg-teal-500 sm:p-3 lg:p-4"
			  href="#Main"
			>
			  <span class="sr-only">Back to top</span>
	  
			  <svg
				xmlns="http://www.w3.org/2000/svg"
				class="h-5 w-5"
				viewBox="0 0 20 20"
				fill="currentColor"
			  >
				<path
				  fill-rule="evenodd"
				  d="M14.707 12.707a1 1 0 01-1.414 0L10 9.414l-3.293 3.293a1 1 0 01-1.414-1.414l4-4a1 1 0 011.414 0l4 4a1 1 0 010 1.414z"
				  clip-rule="evenodd"
				/>
			  </svg>
			</a>
		  </div>
	  
		  <div class="lg:flex lg:items-end lg:justify-between">
			<div>
			  <div class="flex justify-center:justify-start">
				<h1 class="font-bold bg-gradient-to-r from-blue-600 via-blue-500 to-blue-400 inline-block text-transparent bg-clip-text ">paxzdev.xyz</h1>
			  </div>
	  
			  <p class="mx-auto mt-6 max-w-md text-center leading-relaxed text-gray-500 lg:text-left">
				As of 12/23/2023 I am busy with another project, but do not hesitate to contact me for future job inquiries.
			  </p>
			</div>
	  
			<ul
			  class="mt-12 flex flex-wrap justify-center gap-6 md:gap-8 lg:mt-0 lg:justify-end lg:gap-12"
			>
			  <li>
				<a class="text-gray-700 transition hover:text-gray-700/75" href="/About"> About </a>
			  </li>
	  
			  <li>
				<a class="text-gray-700 transition hover:text-gray-700/75" href="/projects"> Projects </a>
			  </li>
	  
			  <li>
				<a class="text-gray-700 transition hover:text-gray-700/75" href="/contact"> Contact </a>
			  </li>
	  
			  <li>
				<a class="text-gray-700 transition hover:text-gray-700/75" href="/projects"> Why choose me </a>
			  </li>
			</ul>
		  </div>
	  

		</div>
	  </footer>
  </Layout>
  